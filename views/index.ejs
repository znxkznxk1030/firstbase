<body>
<div id="loading"><div id="loader"></div></div>
<div id = "main">
    <div id = "left-area">
        <div id = "left-nav-area">
            <div id = "logo-img">
                <a href="/"><img src="/img/layout/logo.png"></a>
            </div>
            <div id = "logo-img-letter">
                <a id="brand" href="#"><img src="/img/layout/logo-letter.png"></a>
            </div>
            <div id="down-link">
                <a href="https://play.google.com/store/apps/details?id=com.yogiwara.firstbase" style="width: 140px; height: auto;">
                    <img src="/img/layout/download.png" style="width: 140px; height: auto;">
                </a>
            </div>
            <div id = "map-search">
                <input type="search" class="form-control" name="search" id="search" placeholder="지도 검색"/>
                <div class="search-button">
                    <button class="btn" type="button">
                        <img src="/img/layout/search.png">
                    </button>
                </div>
            </div>
        </div>
        <div id = "map-area">
            <div id = "map">
            </div>
            <div id = "add-button" onclick="write_button_click();">
                <img src="/img/layout/write-button.png">
            </div>
            <div id = "radar-button" onclick="search_position();">
                <img src="/img/layout/radar.png">
            </div>
        </div>
    </div>
    <div id="right-area">
        <div id = "right-nav-area">
            <div id = "login-register-button">
                <button type="button" class="btn" data-toggle="modal" data-target="#myModal">로그인</button>
            </div>
        </div>
        <div id = "contents-area">
            <div id = "contents">
            </div>
            <div id = "down-menu">
                <div id = "posts-search">
                    <input type="search" class="form-control" name="search" id="search" placeholder="게시글 검색"/>
                    <div class="search-button">
                        <button class="btn" type="button">
                            <img src="/img/layout/search.png">
                        </button>
                    </div>
                </div>
                <div id = "down-filter" style="display: none"><img src="/img/layout/filter.png"></div>
            </div>
        </div>
    </div>
</div>
<!-- Modal -->
<div class="modal fade" id="myModal" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header" style="padding:35px 50px;">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4>Login</h4>
            </div>
            <div class="modal-body" style="padding:40px 50px;">
                <div id = "login-form">
                    <!--<form action="http://ec2-13-124-219-114.ap-northeast-2.compute.amazonaws.com:8080/users/login" method="post" enctype="application/json">-->
                    <div class="form-group" id = "loginId">
                        <label for="id">Email</label>
                        <input type="text" class="form-control" name="id" id="id" placeholder="Enter Email"/>
                    </div>
                    <div class="form-group" id = "loginPw">
                        <label for="password">Password</label>
                        <input type="password" class="form-control" name="password" id="password" placeholder="Enter Password"/>
                    </div>
                    <div class="form-submit">
                        <button type="submit" class="btn btn-success btn-block" id="loginSubmit">Login</button>
                    </div>
                    <!--</form>-->
                </div>
                <div class="social-login">
                    <a class="btn" href="/auth/facebook"><button type="button" class="btn btn-primary">Facebook으로 로그인</button></a>
                </div>
                <div class="social-login">
                    <a class="btn" href="/auth/google"><button type="button" class="btn btn-danger">Google으로 로그인</button></a>
                </div>
                <div class="social-login">
                    <a class="btn" href="/users/signup-form"><button type="button" class="btn btn-warning">Sign Up</button></a>
                </div>
            </div>
            <div class="modal-footer">
                <p>Not a member? <a href="/users/signup-form">Sign Up</a></p>
                <p>Forgot <a href="#">Password?</a></p>
                <button type="submit" class="btn btn-danger btn-default pull-left" data-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>

