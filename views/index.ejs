    <div id="chat-cancel" class="page-cancel">
        <button type="button" class="close" onclick="cancel($('#chatPage'));">&times;</button>
    </div>
    <div id="chat-change">
        <div id="chatLogs"></div>
        <form class="form-inline" id="chat-form">
            <div class="form-group">
                <input type="text" class="form-control" id="msgForm">
            </div>
            <button type="submit" class="btn btn-primary">Send</button>
        </form>
    </div>
