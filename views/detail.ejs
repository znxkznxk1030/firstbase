        <div id = "detail-info">
            <div id = "detail-icon"></div>
            <div id = "detail-text">
                <div id = "detail-title"></div>
                <div id = "detail-others">
                    <div id = "detail-others-top">
                        <div id = "detail-id"></div>
                        <div id = "detail-modified_date"></div>
                    </div>
                    <div id = "detail-others-bot">
                        <div id = "detail-countView">
                            <img src="/img/layout/view.png">
                            <span></span>
                        </div>
                        <div id = "detail-countComments">
                            <img src="/img/layout/comment.png">
                            <span></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id = "detail-images"></div>
        <div id = "detail-content"></div>
        <div id = "detail-prefer">
            <div id = "detail-like">
                <div class="detail-prefer-img" id = "detail-like-img">
                    <img src="/img/layout/heart.png">
                </div>
                <div class="detail-prefer-text">
                    <div class="detail-prefer-count" id="detail-like-count"></div>
                    <div class="detail-prefer-word">인정</div>
                </div>
            </div>
            <div id= "detail-dislike">
                <div class="detail-prefer-img" id = "detail-dislike-img">
                    <img src="/img/layout/broken-heart.png">
                </div>
                <div class="detail-prefer-text">
                    <div class="detail-prefer-count" id="detail-dislike-count"></div>
                    <div class="detail-prefer-word">노인정</div>
                </div>
            </div>
        </div>
    </div>
    <div id="detail-userInfo-area">
        <div id = "detail-profileImg">
        </div>
        <div id = "detail-follow">
            <button type="submit" class="btn">팔로우</button>
